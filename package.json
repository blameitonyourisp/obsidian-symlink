{
	"name": "obsidian-symlink",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"type": "module",
	"main": "./dist/main.js",
	"scripts": {
		"build": "npm run build:dev",
		"build:dev": "node admin/scripts/build.js",
		"build:prod": "tsc && node admin/scripts/build.js",
		"docs": "typedoc --options ./admin/config/typedoc.config.json",
        "lint": "npm run lint:check",
        "lint:check": "eslint ./src ./admin --cache --cache-location ./.cache/eslint.json",
        "lint:fix": "eslint ./src ./admin --cache --cache-location ./.cache/eslint.json --fix",
        "lint:fix-dry": "eslint ./src ./admin --cache --cache-location ./.cache/eslint.json --fix-dry",
		"postversion": "git push --follow-tags",
		"preversion": "npm run build:prod",
		"version": "node admin/scripts/version.js && git add manifest.json versions.json",
		"types": "tsc"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"symlink"
	],
	"author": "James Reid",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.6.3",
		"@typescript-eslint/eslint-plugin": "6.7.2",
		"@typescript-eslint/parser": "6.7.2",
		"builtin-modules": "3.3.0",
		"docdash": "^2.0.2",
		"esbuild": "0.19.3",
		"eslint": "^8.49.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-regex": "^1.10.0",
		"jest": "^29.7.0",
		"jsdoc": "^4.0.2",
		"obsidian": "latest",
		"tslib": "2.6.2",
		"typedoc": "^0.25.1",
		"typescript": "^5.2.2"
	}
}
